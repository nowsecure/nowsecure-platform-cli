{
  "name": "@nowsecure/github-snapshot",
  "version": "1.2.1",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "author": "NowSecure",
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "yarn run clean && genversion -d --es6 --semi src/version.ts",
    "build": "yarn prebuild && tsc -p tsconfig.production.json",
    "debugbuild": "yarn prebuild && tsc --sourceMap",
    "lint": "eslint src && prettier --check **/*.ts",
    "format": "prettier --write .",
    "test": "jest"
  },
  "engines": {
    "node": ">=20"
  },
  "files": [
    "/lib"
  ],
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/lodash": "4.14.184",
    "@types/node": "17.0.7",
    "@types/sarif": "2.1.4",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "eslint": "8.6.0",
    "genversion": "3.0.2",
    "jest": "27.5.1",
    "nock": "13.2.4",
    "prettier": "2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "27.1.4",
    "typescript": "4.9.3"
  },
  "dependencies": {
    "@nowsecure/platform-lib": "file:../lib"
  }
}